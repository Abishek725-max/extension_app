{
  "name": "my-chrome-extension",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:webpack": "webpack --config webpack.config.js",
    "ext-build": "EXTENSION_BUILD=TRUE next build && rm -rf ./extension/* && rm -rf ./dist/* && npm run build:webpack && mv ./build/_next ./build/next && mv ./build/* ./extension/ && mv ./dist/* ./extension/ && cd ./extension && grep -rli '_next' * | xargs -I@ sed -i '' 's|/_next|/next|g' @ && cp .env.local ./extension/ || echo '.env.local not found, skipping move.'",
    "move-env": "cp .env.local ./extension/"
  },
